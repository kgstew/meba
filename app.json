{
  "expo": {
    "name": "meba-app",
    "slug": "meba-app",
    "version": "1.0.0",
    "orientation": "landscape",
    "scheme": "meba",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
